@model Web.Providers.DynamicRowAndColumns
@{
    ViewData["Title"] = "callAnyProcedure";
}


<div class="row">
    <div class="col-md-12">
        <table class="table table-striped table-responsive-sm">
            <thead>
                <tr>
                    @{
                        foreach (var cols in Model.Cols)
                        {
                            <th>@cols</th>
                        }
                    }
                    <td></td>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model.Rows)
                {
                    <tr>
                        @foreach (var data in item)
                        {
                            <td>@data</td>
                        }
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>

